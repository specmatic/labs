{
  "http-request": {
    "path": "/orders",
    "method": "POST",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "productid": 1,
      "count": 100
    }
  },
  "http-response": {
    "status": 201,
    "body": {
      "id": "$match(exact:1)"
    },
    "status-text": "Created",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}