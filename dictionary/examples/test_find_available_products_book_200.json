{
  "http-request": {
    "path": "/findAvailableProducts",
    "method": "GET",
    "query": {
      "from-date": "2026-02-15",
      "to-date": "2026-02-15",
      "type": "book"
    },
    "headers": {
      "pageSize": "10"
    }
  },
  "http-response": {
    "status": 200,
    "body": {
      "id": "$match(exact:1)",
      "name": "$match(exact:Harry Potter)",
      "type": "$match(exact:book)",
      "inventory": "$match(exact:100)",
      "createdOn": "$match(exact:2026-02-15)"
    },
    "status-text": "OK",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}